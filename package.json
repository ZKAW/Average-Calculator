{
  "name": "grade_calculator",
  "version": "1.0.0",
  "description": "Grade calculator",
  "main": "js/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build32": "electron-packager ./ \"Grade calculator\" -p --platform=win32 --arch=ia32 --overwrite --out \"build\"",
    "build64": "electron-packager ./ \"Grade calculator\" -p --platform=win32 --arch=x64 --overwrite --out \"build\"",
    "buildall": "npm run build32 && npm run build64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZKAW/grade_calculator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZKAW/grade_calculator/issues"
  },
  "homepage": "https://github.com/ZKAW/grade_calculator#readme",
  "devDependencies": {
    "electron": "^11.2.2",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.0"
  },
  "dependencies": {
    "ext-ip": "^0.3.9",
    "node-iplocate": "^1.0.2"
  }
}
