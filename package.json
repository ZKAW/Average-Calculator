{
  "name": "average_calculator",
  "version": "1.0.0",
  "description": "Average calculator",
  "main": "js/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build32": "electron-packager ./ \"Average calculator\" -p --platform=win32 --arch=ia32 --overwrite --out \"build\"",
